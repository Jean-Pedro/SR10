<html lang="fr">
    <head>
        <meta charset="utf-8">
        <title>Candidater</title>
        <link rel="stylesheet" href="/stylesheets/connexion.css">
    </head>
    <body>
        <div id="container_2">
            <div id="titre">
                <h1>Candidater</h1>
            </div>
            <div id="case">
                <div id="column_g">
                    <div id="formulaire">
                        <form action="validation_candidat.html" method="post">
                            <% pieces.forEach((piece)=> { %>
                                <div id="label_form">
                                    <label for="CV"><%= piece%></label> <br>
                                </div>
                                <div id="input_form">
                                    <input type="file" id="CV" name="CV" placeholder="<%= piece.fichier%>" required>
                                </div>
                                <% }) %>
                            <button id="submit" type="submit">Candidater</button>
                        </form>
                    </div>
                </div>
                <div id="column_d">
                    <div>
                        <div class="entreprise">
                        
                        </div>
                    </div>
                </div>
            </div>
        </div>

        
    
    
    </body>
</html>