<html>
    <head>
        <meta charset="utf-8">
        <title>Creation organisation</title>
        <link rel="stylesheet" href="/stylesheets/connexion.css">
    </head>
    <body>
        <div id="container">
            <div id="titre">
                <h1>Création d'organisation</h1>
            </div>
            <form action="" method="post">
                <div id="case">
                    <div id="column_g">
                        <div id="formulaire">
                            <div id="label_form">
                                <label for="siren">Siren</label> <br>
                            </div>
                            <div id="input_form">
                                <input type="number" id="siren" name="siren" placeholder="Entrez le numéro de siren" required>
                            </div>
                            <div id="label_form">
                                <label for="nom">Nom</label> <br>
                            </div>
                            <div id="input_form">
                                <input type="text" id="nom" name="nom" placeholder="Entrez le nom" required>
                            </div>
                            <div id="label_form">
                                <label for="num">Numéro de rue du siège</label> <br>
                            </div>
                            <div id="input_form">
                                <input type="number" id="num" name="num" placeholder="Entrez le numéro de rue du siège" required>
                            </div>
                            <div id="label_form">
                                <label for="rue">Nom de la rue</label> <br>
                            </div>
                            <div id="input_form">
                                <input type="text" id="rue" name="rue" placeholder="Entrez la rue du siège" required>
                            </div>
                            <div id="label_form">
                                <label for="ville">Ville</label> <br>
                            </div>
                            <div id="input_form">
                                <input type="text" id="ville" name="ville" placeholder="Entrez la ville du siège" required>
                            </div>
                            <div id="label_form">
                                <label for="code_postal">Code postal du siège</label> <br>
                            </div>
                            <div id="input_form">
                                <input type="text" id="code_postal" name="code_postal" placeholder="Entrez le code postal du siège" required>
                            </div>
                            <div id="label_form">
                                <label for="type">Type d'organisation</label> <br>
                            </div>
                            <div id="input_form">
                                <input type="text" id="type" name="type" placeholder="Entrez le type d'organisation" required>
                            </div>
                            <div id="label_form">
                                <label for="logo">Logo</label> <br>
                            </div>
                            <div id="input_form">
                                <input type="file" id="logo" name="logo" required>
                            </div>
                            <button id="submit" type="submit">Valider</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </body>
</html>